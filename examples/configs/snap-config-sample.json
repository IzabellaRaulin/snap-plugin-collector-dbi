{
    "control": {
        "cache_ttl": "5s"
    },
    "scheduler": {
        "default_deadline": "5s",
        "worker_pool_size": 5
    },
    "plugins": {
        "collector": {
            "dbi": {
                "all": {
			"setfile": "$SNAP_DBI_PLUGIN_DIR/examples/configs/setfiles/sql_example.json"
                }
            }
        },
        "publisher": {
            "influxdb": {
                "all": {
                    "server": "xyz.local",
                    "password": "$password"
                }
            }
        },
        "processor": {}
    }
}
